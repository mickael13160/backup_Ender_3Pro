[stepper_x]
step_pin: PB8
dir_pin: !PB9
enable_pin: !PA8
microsteps: 16
#rotation_distance: 40
rotation_distance: 39.80
full_steps_per_rotation: 200
endstop_pin: ^PA1
position_endstop: 0
position_min: 0
position_max: 250
homing_speed: 50

[tmc2209 stepper_x]
uart_pin: PA3
tx_pin: PA2
uart_address: 0
run_current: 0.500
#hold_current: 0.500
stealthchop_threshold: 999999

[stepper_y]
step_pin: PB2
dir_pin: !PB3
enable_pin: !PB1
microsteps: 16
#rotation_distance: 40
rotation_distance: 39.80
full_steps_per_rotation: 200
endstop_pin: ^PB4
position_endstop: 0
position_min: -2
position_max: 241
homing_speed: 50

[tmc2209 stepper_y]
uart_pin: PA3
tx_pin: PA2
uart_address: 2
run_current: 0.500
#hold_current: 0.500
stealthchop_threshold: 999999

[stepper_z]
step_pin: PC0
dir_pin: PC1
enable_pin: !PC2
microsteps: 16
rotation_distance: 10
#rotation_distance: 9.95
full_steps_per_rotation: 200
endstop_pin: probe:z_virtual_endstop
#endstop_pin: ^PA15
#position_endstop: 4
position_min: -8
position_max: 250
homing_speed: 25
homing_retract_dist: 5.0
homing_retract_speed: 10
second_homing_speed: 5

[tmc2209 stepper_z]
uart_pin: PA3
tx_pin: PA2
uart_address: 1
run_current: 0.500
#hold_current: 0.500
stealthchop_threshold: 999999